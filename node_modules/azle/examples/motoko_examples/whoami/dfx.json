{
    "canisters": {
        "whoami": {
            "type": "custom",
            "main": "src/index.ts",
            "candid": "src/index.did",
            "build": "npx azle whoami",
            "wasm": ".azle/whoami/whoami.wasm",
            "gzip": true,
            "declarations": {
                "output": "test/dfx_generated/whoami",
                "node_compatibility": true
            },
            "env": ["AZLE_TEST_FETCH"],
            "assets": [["src/index.did", "src/whoami.did"]]
        }
    }
}
